{
  "version": "5.0.0",
  "description": "Demo app used in Delicious Insightsâ€™ Modern Web Apps training course",
  "author": "Christophe Porteneuve <christophe@delicious-insights.com>",
  "license": "No-License (see LICENSE file)",
  "scripts": {
    "build": "react-scripts build",
    "deploy": "netlify deploy --prod",
    "doc": "rimraf doc && npx groc",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "start:all": "npm-run-all --parallel --print-label start start:api",
    "start:api": "node tools/dev-server.mjs",
    "start:poll": "cross-env CHOKIDAR_USEPOLLING=true npm start",
    "test": "react-scripts test",
    "test:cov": "npm test -- --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "prettier": {
    "jsxSingleQuote": true,
    "semi": false,
    "singleQuote": true
  },
  "deliciousImportSort": {
    "customGroups": "@material-ui",
    "mainOrder": [
      "absolute",
      "relative",
      "bare"
    ]
  },
  "importSort": {
    ".js": {
      "style": "delicious"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "src/**/*.stories.js"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@redux-offline/redux-offline": "^2.6.0",
    "airbnb-prop-types": "^2.16.0",
    "bson-objectid": "^1.3.1",
    "date-fns": "^2.17.0",
    "localforage": "^1.9.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "reduce-reducers": "^1.0.4",
    "redux": "^4.0.5",
    "web-vitals": "^1.1.0",
    "workbox-background-sync": "^6.1.0",
    "workbox-broadcast-update": "^6.1.0",
    "workbox-cacheable-response": "^6.1.0",
    "workbox-core": "^6.1.0",
    "workbox-expiration": "^6.1.0",
    "workbox-google-analytics": "^6.1.0",
    "workbox-navigation-preload": "^6.1.0",
    "workbox-precaching": "^6.1.0",
    "workbox-range-requests": "^6.1.0",
    "workbox-routing": "^6.1.0",
    "workbox-strategies": "^6.1.0",
    "workbox-streams": "^6.1.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.17",
    "@storybook/theming": "^6.1.17",
    "@testing-library/dom": "^7.28.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-style-delicious": "^1.0.2",
    "morgan": "^1.10.0",
    "netlify-cli": "^3.5.0",
    "npm-run-all": "^4.1.5"
  }
}
